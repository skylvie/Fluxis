{
    "name": "fluxis",
    "version": "1.0.0",
    "description": "A Discord selfbot for extending GDMs",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "lint": "biome lint src",
        "lint:fix": "biome lint --write src",
        "format": "biome format --write src",
        "check": "biome check --write src",
        "cloc": "cloc . --exclude-dir=dist,node_modules --exclude-list-file=pnpm-lock.yaml"
    },
    "author": "Sylvie Rain",
    "license": "MIT",
    "packageManager": "pnpm@10.28.1",
    "devDependencies": {
        "@biomejs/biome": "^2.3.14",
        "@types/better-sqlite3": "^7.6.13",
        "@types/node": "^25.2.2",
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "better-sqlite3": "^12.6.2",
        "debug": "^4.4.3",
        "discord.js-selfbot-v13": "^3.7.1"
    }
}
